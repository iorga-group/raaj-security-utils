{
  "name": "raaj-security-utils",
  "version": "1.4.0",
  "homepage": "https://github.com/iorga-group/raaj-security-utils",
  "authors": [
    "iORGA GROUP"
  ],
  "description": "Restless Architecture for AngularJS - Security Utils",
  "main": "./src/raaj-security-utils.js",
  "dependencies": {
  	"angular": "~1.2.17",
    "cryptojslib": "~3.1.2"
  },
  "overrides": {
    "cryptojslib": {
      "main": [
      	"components/core.js",
      	"components/md5.js",
      	"components/sha1.js",
      	"components/hmac.js",
        "components/enc-base64.js",
        "components/evpkdf.js",
        "components/cipher-core.js",
        "components/aes.js",
        "components/pbkdf2.js"
      ]
    }
  },
  "devDependencies": {
    "angular-mocks": "~1.2.17"
  },
  "keywords": [
    "raaj",
    "security-utils",
    "restless"
  ],
  "license": "LGPLv3",
  "ignore": [
    "test"
  ]
}
